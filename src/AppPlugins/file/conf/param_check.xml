<?xml version="1.0" encoding="utf-8"?>
<rest_define>
    <!--
    param_type:定义一种参数的校验规则，仅指示某种规则
        必选子标签:id、type、cond
        id:标识一种检验规则
        type:string/int/internal
        cond:条件
            string:正则表达式
            length:字符串长度
            int:(0,10)/(0,)/(,10)
            internal:自定义的过滤器
    -->
    <param_type_define>
        <param_type id="COMMON_VALUE_UINT64">
            <type>int</type>
            <cond>[0,18446744073709551615]</cond>
        </param_type>
        <param_type id="COMMON_VALUE_UINT32">
            <type>int</type>
            <cond>[0,4294967295]</cond>
        </param_type>
        <param_type id="COMMON_VALUE_10000">
            <type>int</type>
            <cond>[0,10000]</cond>
        </param_type>
        <param_type id="IPV4_OR_IPV6_STR">
            <type>string</type>
            <cond>^[0-9a-zA-Z.:,\[\]\/%]*$</cond>
        </param_type>
        <param_type id="COMMON_PORT">
            <type>int</type>
            <cond>[0,65535]</cond>
        </param_type>
        <param_type id="COMMON_BOOL_VALUE">
            <type>string</type>
            <cond>^(false|true|0|1)$</cond>
        </param_type>
        <param_type id="COMMON_ENUM_VALUE">
            <type>int</type>
            <cond>[0,100]</cond>
        </param_type>
        <param_type id="COMMON_ID">
            <type>string</type>
            <cond>^[a-zA-Z_0-9-]{0,64}$</cond>
        </param_type>
        <param_type id="JOB_NAME">
            <type>string</type>
            <cond>^[a-zA-Z_0-9-./]{0,64}$</cond>
        </param_type>
        <param_type id="ABSOLUTE_PATH">
            <type>string</type>
            <cond>(^[A-z]:(\\([^:*?&quot;&lt;&gt;|]{1,2048}))+$)|(^[A-z]:\\$)|(^(/[^|;&amp;$&lt;&gt;`!+\[\]]{0,2048})+$)|(^/$)</cond>
        </param_type>
        <param_type id="PAGE_SIZE">
            <type>int</type>
            <cond>[0,1000]</cond>
        </param_type>
        <param_type id="CHINESE_ENGLISH_NAME">
            <type>string</type>
            <cond>^[\u4e00-\u9fa5a-zA-Z_0-9-./]{0,64}$</cond>
        </param_type>
        <param_type id="SCRIPT_PATH">
            <type>string</type>
            <cond>^(.+)[.](sh|bat)$|^$</cond>
        </param_type>
        TIMESTAMP
        <param_type id="TIMESTAMP">
            <type>int</type>
            <cond>[0,9999999999999]</cond>
        </param_type>
        <param_type id="SNAPSHOT_ID">
            <type>string</type>
            <cond>^[a-zA-Z_0-9-@]{0,254}$</cond>
        </param_type>
        <param_type id="SNAPSHOT_PARENT_NAME">
            <type>string</type>
            <cond>^[a-zA-Z_0-9-.]{0,254}$</cond>
        </param_type>
        <param_type id="COPY_NAME">
            <type>string</type>
            <cond>^[a-zA-Z_0-9-.]{0,512}$</cond>
        </param_type>
        <param_type id="RESOURCE_FILTER_MODE">
            <type>string</type>
            <cond>^(INCLUDE|EXCLUDE|include|exclude)$</cond>
        </param_type>
        <param_type id="RESTORE_MODE">
            <type>string</type>
            <cond>^(RemoteRestore|LocalRestore)$</cond>
        </param_type>
        <param_type id="FILE_PLUGIN_SUB_JOB_INFO_CONTROL_FILE_PATH">
            <type>string</type>
            <cond>(^(\\([^\\/:*?&quot;&lt;&gt;|]{1,255}))+$)|(^\\$)|(^(/[^/|;&amp;$&lt;&gt;`\\!+\[\]]{0,255})+$)|(^/$)</cond>
        </param_type>
        <param_type id="LVM_VOLUME_PATH">
            <type>string</type>
            <cond>^/dev/mapper/[a-zA-Z_0-9-.]{0,1024}$</cond>
        </param_type>
        <param_type id="RESTORE_OPTION">
            <type>string</type>
            <cond>^(OVERWRITING|SKIP|REPLACE)$</cond>
        </param_type>
        <param_type id="NOT_VERIFICATION">
            <type>length</type>
            <cond>[0,9223372036854775807]</cond>
        </param_type>
    </param_type_define>
    <!--
        struct_define: 定义一类消息体的校验规则，其中包含该消息体中各参数所使用的param_type的组合
        param: 对应一个参数的检查，一个参数可能有多种检验规则，可以包含多个usetype
        subStruct: 当参数为结构体类型时，使用subStruct代替param
        必选子标签：name、usetype
        name: 结构体成员变量的名称
        usetype: 成员变量使用的参数校验规则
    -->
