{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "https://example.com/product.schema.json",
  "title": "ProtectedResource",
  "description": "Protected Resource Info",
  "type": "object",
  "properties": {
    "uuid": {
      "description": "The uuid of resource",
      "type": "string"
    },
    "createdTime": {
      "description": "The created time of the resource",
      "type": "string"
    },
    "protectionStatus": {
      "description": "The protectionStatus of the resource",
      "type": "integer"
    },
    "userId": {
      "description": "The userId of the owner of the resource",
      "type": "string"
    },
    "extendInfo": {
      "type": "object",
      "properties": {
        "key1": {
          "type": "string"
        },
        "key2": {
          "type": "string"
        }
      }
    }
  },
  "required": [ "uuid", "name", "type", "subType"]
}