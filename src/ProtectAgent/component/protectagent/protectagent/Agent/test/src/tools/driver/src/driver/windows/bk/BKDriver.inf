
[Version]
Signature = "$Windows NT$"
Class     = Volume
ClassGUID = {71a27cdd-812a-11d0-bec7-08002be2092f}
Provider  = %hw%
DriverVer = 11/01/2018,8.1.0000.0
CatalogFile=%DriverName%.cat



[DestinationDirs]
DefaultDestDir = 12

[SourceDisksFiles]
BKDriver.sys=1

[SourceDisksNames]
1 = %diskid1%,,,


[DefaultUninstall.NT]
DelFiles = BKDriver.DeleteDriverFiles
DelReg   = BKDriver.DelReg

[BKDriver.DeleteDriverFiles]
%DriverName%.sys,,,0x00010001

[BKDriver.DelReg]
HKLM, System\CurrentControlSet\Control\Class\{71a27cdd-812a-11d0-bec7-08002be2092f}, UpperFilters, 0x00018002, %DriverName%
HKLM, System\CurrentControlSet\Control\Class\{533c5b84-ec70-11d2-9505-00c04f79deaf}, UpperFilters, 0x00018002, %DriverName%


[DefaultInstall.NT]
CopyFiles = BKDriver.CopyDriverFiles
Addreg    = BKDriver.AddReg

[BKDriver.AddReg]
HKLM, System\CurrentControlSet\Control\Class\{71a27cdd-812a-11d0-bec7-08002be2092f}, UpperFilters, 0x00010008, %DriverName%
HKLM, System\CurrentControlSet\Control\Class\{533c5b84-ec70-11d2-9505-00c04f79deaf}, UpperFilters, 0x00010008, %DriverName%


[BKDriver.CopyDriverFiles]
%DriverName%.sys,,,0x00002000



[DefaultUninstall.NT.services]
DelService = %DriverName%,0x00000200,System

[DefaultInstall.NT.Services]
AddService = %DriverName%, , BKDriver.Service.Install

[BKDriver.Service.Install]
DisplayName    = %service_desc%
ServiceType    = 1
StartType      = 0
ErrorControl   = 1
ServiceBinary  = %12%\%DriverName%.sys
LoadOrderGroup = "PnP Filter"



[Strings]

hw         = "Huawei Technologies Co.,Ltd."
service_desc = "Volume Filter Driver"
DriverName = "BKDriver"
diskid1      = "HuaWei Technologies. Installation Disk #1 (BKDriver)"
